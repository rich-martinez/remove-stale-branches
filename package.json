    "inquirer-autocomplete-prompt": "^1.0.1",
    "inquirer-checkbox-plus-prompt": "^1.0.1",
    "simple-git": "^1.106.0"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "standard": "^12.0.1"
  },
  "standard": {
    "globals": [ "jest", "expect", "test" ]
